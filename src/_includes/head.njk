<meta charset="UTF-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<title>Newer: {{ title }}</title>
<link rel="stylesheet" href="{{ '/css/application.css' | url }}">
{% include "favicon.njk" %}
<meta name="description" content="{{ description or renderData.description }}" />
<link rela="canonical" href="{{ page.url }}" />
<meta name="robots" content="index, follow" />
<meta property="og:title" content="{{ title or renderData.title or metadata.title }}" />
<meta property="og:url" content="{{ page.url }}" />
<meta property="og:description" content="{{ description or renderData.description }}" />
<meta property="og:type" content="article" />
<meta name="twitter:card" content="summary_large_image"/>
<meta name="twitter:site" content="@mafulprayoga"/>
<meta name="twitter:creator" content="@mafulprayoga"/>
{% if opengrapgh %}
  <meta property="og:image" content="/assets/og/{{ title | slugify }}.jpg" />
{% endif %}
